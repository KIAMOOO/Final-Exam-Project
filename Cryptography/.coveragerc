[run]
source = src
omit = 
    */web/*
    */auth/*
    */__pycache__/*
    */tests/*
    */htmlcov/*
    setup.py
    */main.py
    */cryptovault.py
    */src/main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

